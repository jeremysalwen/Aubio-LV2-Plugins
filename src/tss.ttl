@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .

<urn:aubio:plugins:tss> a lv2:Plugin ;
    a lv2:AnalyserPlugin ;
    doap:name "Transient Steady State Separator" ;
    doap:licence <http://usefulinc.com/doap/licenses/gpl> ;

    lv2:port [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 0 ;
	lv2:symbol "threshold" ;
	lv2:name "Threshold" ;
	lv2:default 0.02 ;
	lv2:minimum 0 ;
	lv2:maximum 1 ;
    ] , [
	a lv2:AudioPort ;
	a lv2:InputPort ;
	lv2:index 1 ;
	lv2:symbol "in" ;
	lv2:name "in" ;
    ] , [
	a lv2:AudioPort ;
	a lv2:OutputPort ;
	lv2:index 2 ;
	lv2:symbol "transient" ;
	lv2:name "Transient" ;
    ] , [
	a lv2:AudioPort ;
	a lv2:OutputPort ;
	lv2:index 3 ;
	lv2:symbol "steady" ;
	lv2:name "Steady State" ;
    ] , [
	a lv2:ControlPort ;
	a lv2:OutputPort ;
	lv2:index 4 ;
	lv2:portProperty lv2:reportsLatency;
	lv2:symbol "latency" ;
	lv2:name "Latency" ;
    ] .